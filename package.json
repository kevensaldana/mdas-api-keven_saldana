{
  "name": "pokedex",
  "version": "1.0.0",
  "description": "Pokedex for Software Design II (MDAS La Salle)",
  "preferGlobal": true,
  "bin": "./build/src/apps/console/Start.js",
  "scripts": {
    "dev": "nodemon",
    "start": "NODE_ENV=dev node build/src/apps/api/Start",
    "cli": "node -r tsconfig-paths/register -r ts-node/register ./src/apps/console/Start.ts",
    "build": "npm run build:clean && npm run build:tsc && npm run build:di",
    "build:di": "copy 'src/**/*.{json,yaml,html,png}' build/src",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:clean": "rm -r build; exit 0",
    "test": "PORT=5002 NODE_ENV=test jest",
    "test:win": "set PORT=5002 && set NODE_ENV=test&& jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/clear": "^0.1.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/figlet": "^1.5.4",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "@types/pubsub-js": "^1.8.3",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.2",
    "prisma": "^3.7.0",
    "shallow-equal-object": "^1.1.1",
    "supertest": "^6.2.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@prisma/client": "^3.7.0",
    "amqp-ts": "^1.8.0",
    "axios": "^0.24.0",
    "chalk": "^5.0.0",
    "clear": "^0.1.0",
    "compression": "^1.7.4",
    "copy": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.1",
    "figlet": "^1.5.2",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "morgan": "^1.10.0",
    "node-dependency-injection": "^2.7.1",
    "pubsub-js": "^1.9.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yargs": "^17.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
